pipeline {
    agent none  

    stages {
        stage('build') {
            steps {
                echo "hello world"
                script {
                    def browsers = ['chrome', 'firefox']
                    for (int i = 0; i < browsers.size(); ++i) {
                        echo "Testing the ${browsers[i]} browser"
                    }
            }
        }
     stage('test') {
            steps {
                 
                echo "hello world test"
                
            }    
    }
            stage('deploy') {
            steps {
                
                
                echo "hello world deploy"
                
        }
    }    
  }       
} 
